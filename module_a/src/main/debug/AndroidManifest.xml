<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cc.rptang.module_a">

    <application
        android:name="debug.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/module_a_ic_launcher"
        android:label="@string/module_a_app_name"
        android:roundIcon="@mipmap/module_a_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/module_a_AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>